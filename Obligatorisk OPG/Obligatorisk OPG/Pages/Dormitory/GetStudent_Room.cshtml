@page
@model Obligatorisk_OPG.Pages.Dormitory.GetStudent_RoomModel
@{
}
<h4 style="color:red"> List of Students in Rooms </h4>
<table class="table">
    <thead>
        <tr>
            <th> RoomNo </th>
            <th> Dormitory Number </th>
            <th> StudentNo </th>
            
        </tr>
    </thead>
    <tbody>

        @if (Model.Room_Student != null)

            @foreach (var item in Model.Room_Student)
            {
                <tr>
                    <td> @item.RoomNo</td>
                    
                    <td> @item.DormitoryNo</td>
                    <td> @item.StudentNo </td>
                    
                </tr>
            }
</table>